{
  "name": "gfmjs",
  "version": "1.1.0",
  "description": "JavaScript test runner for GitHub Flavored Markdown files",
  "main": "src/index.js",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "gfmjs": "./bin/gfmjs.js"
  },
  "scripts": {
    "test": "shopt -s nullglob && ls ./{src,test}/**/*.{spec,test}.js | xargs -I file -exec node file",
    "test:docs": "./bin/gfmjs.js ./README.md"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "zora": "^4.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@m59/use-tmp-dir": "^1.0.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "builtin-modules": "^3.1.0",
    "into-stream": "^6.0.0",
    "magic-string": "^0.25.7",
    "markdown-it": "^11.0.1",
    "promisify-child-process": "^4.1.1",
    "regex-fun": "^2.0.2",
    "replace-ext": "^2.0.0",
    "rollup": "^2.29.0",
    "sade": "^1.7.4",
    "source-map": "^0.7.3"
  }
}
